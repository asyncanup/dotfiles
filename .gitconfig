[alias]
  s = status -s
  f = fetch
  fap = fetch --all --prune
  cl = clone
  d = diff
  db = !git diff $1..HEAD
  dc = diff --cached
  di = diff --no-index
  dm = diff master..HEAD
  dms = diff master..HEAD --stat
  dmm = !git diff $(git merge-base master HEAD)..HEAD
  sh = show
  cfs = diff --name-only --diff-filter=U
  co = checkout
  com = checkout master
  cob = checkout -b
  a = add
  ap = add -p
  st = stash
  stl = stash list
  stp = stash pop
  sts = stash show -p
  stss = stash show -p --stat
  std = stash drop
  stdi = !git stash drop stash@{$1}
  stc = stash clear
  apl = apply
  c = commit --verbose
  cm = commit -m
  cam = commit -a -m
  ca = commit --verbose -a
  cc = commit --verbose --amend
  cu = config user.name
  ce = config user.email
  cp = cherry-pick
  p = push
  pt = push --tags
  pf = push --force
  pdo = push -d origin
  t = tag
  td = tag -d
  tdo = push --delete origin
  b = branch
  ba = branch -a
  bd = branch --delete
  bD = branch -D
  bmm = branch -a --merged master
  bnm = branch -a --no-merged master
  l = log --pretty=format:\"%Cgreen%ad %Creset%s%Cgreen%d %Cblue- %an %Cgreen%h\" --graph --date=short
  lp = log -p
  hist = !git log $(git merge-base master HEAD)..HEAD --pretty=format:"%B"
  r = reset
  rs = reset --soft
  rh = reset --hard
  rsh = reset --soft HEAD^
  rhh = reset --hard HEAD
  rp = reset -p
  rev = rev-parse
  cat = cat-file -p
  pl = pull --rebase
  plm = pull --rebase origin master
  type = cat-file -t
  m = merge --no-ff
  mf = merge --ff
  rb = rebase
  rbi = rebase -i
  rbim = rebase -i master
  rbm = rebase master
  rba = rebase --abort
  rbc = rebase --continue
  rbs = rebase --skip
  bs = bisect
  bss = bisect start
  bsg = bisect good
  bsb = bisect bad
  bsr = bisect reset

[core]
  excludesfile = ~/.gitignore
[push]
  default = current
[merge]
  renamelimit = 3000
